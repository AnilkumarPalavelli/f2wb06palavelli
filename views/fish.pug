extends layout

block content
  h1= title
  - var fish=[{fish_type:"Salmon",fish_size:5,fish_cost:20},{fish_type:"Tuna",fish_size:3,fish_cost:30},{fish_type:"Cod",fish_size:8,fish_cost:50}]
  div(class="container-fluid")
  div(class="row")
   each f in fish
    div(class="col-sm-2") #{f.fish_type}, #{f.fish_size}, #{f.fish_cost}
   